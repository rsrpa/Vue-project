{
  "name": "@codehq/prickles-client-documents-expenses",
  "version": "0.0.1",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/prickles-client-documents-expenses.umd.min.js",
  "scripts": {
    "build": "vue-cli-service build --target lib --name prickles-client-documents-expenses src/index.js",
    "build:local": "vue-cli-service build --target lib --name prickles-client-documents-expenses src/index.js && npm pack",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve",
    "plop": "plop",
    "serve:ci": "vue-cli-service serve --mode production",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o docs/storybook",
    "storybook:docs": "build-storybook --docs",
    "cy:ci": "cypress run --headless",
    "cy:open": "cypress open",
    "cy:verify": "cypress verify",
    "test:e2e:ci": "start-server-and-test serve:ci http-get://localhost:8080 cy:ci",
    "test:unit": "vue-cli-service test:unit tests/specs/**/*.spec.js",
    "test:unit:ci": "nyc vue-cli-service test:unit tests/specs/**/*.spec.js --require tests/mocha.config.js --reporter mocha-junit-reporter --reporter-options mochaFile=./test-results/unit-test-results.xml",
    "test:unit:dev": "vue-cli-service test:unit tests/specs/**/*.spec.js -w"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "core-js": "^3.6.4",
    "moxios": "^0.4.0",
    "vue": "^2.6.11",
    "vue-friendly-iframe": "^0.18.0",
    "vue-router": "^3.2.0",
    "vuetify": "^2.2.11",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-react": "^7.6.3",
    "@cypress/code-coverage": "^3.8.3",
    "@mdi/font": "^4.5.95",
    "@storybook/addon-a11y": "^6.0",
    "@storybook/addon-actions": ">=6.0.28",
    "@storybook/addon-docs": ">=6.0.28",
    "@storybook/addon-knobs": ">=6.0.28",
    "@storybook/addon-links": ">=6.0.28",
    "@storybook/addon-notes": "^5.3",
    "@storybook/addon-viewport": "^6.0",
    "@storybook/addons": "^6.0",
    "@storybook/vue": ">=6.0.28",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "^4.5.8",
    "@vue/cli-plugin-router": "^4.5.8",
    "@vue/cli-plugin-unit-mocha": "^4.5.8",
    "@vue/cli-plugin-vuex": "^4.3.1",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-preset-vue": "^2.0.2",
    "chai": "^4.1.2",
    "cypress": "^5.5.0",
    "eslint": "^6.7.2",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "istanbul-instrumenter-loader": "^3.0.1",
    "js-beautify": "^1.13.0",
    "mocha-junit-reporter": "^2.0.0",
    "null-loader": "^4.0.1",
    "nyc": "^15.1.0",
    "plop": "^2.7.3",
    "prism-react-renderer": "^0.1.7",
    "prismjs": "^1.17.1",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "start-server-and-test": "^1.11.5",
    "vue-cli-plugin-storybook": "~2.0.0",
    "vue-cli-plugin-vuetify": "^2.0.6",
    "vue-cli-plugin-vuetify-storybook": "~0.2.0",
    "vue-storybook": "^1.2.0",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.6.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "include": [
      "src/**/*.{js,vue}"
    ],
    "exclude": [
      "src/index.js",
      "src/main.js",
      "src/components/index.js",
      "src/plugins/vuetify.js",
      "src/router/index.js",
      "src/store/index.js",
      "src/store/modules/index.js",
      "src/stories/**/*.js",
      "src/views/index.js"
    ],
    "reporter": [
      "text",
      "text-summary",
      "cobertura"
    ],
    "extension": [
      ".js",
      ".vue"
    ],
    "cache": true,
    "all": true
  }
}
